<section>
  <h1>New Ticket</h1>
  <form method="post" action="/tickets" class="card">
    <label>Subject
      <input type="text" name="subject" required />
    </label>
    <label>Description
      <textarea name="description" rows="6" required></textarea>
    </label>
    <button type="submit" class="btn primary">Create Ticket</button>
  </form>
</section>
